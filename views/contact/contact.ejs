<% include ../partials/header.ejs %>

  <div id="contactList" class="container">
    <div class="row align-items-center justify-content-center">
      <div class="col-md-3">
        <img src="/images/jeff.jpg" class="contact-img rounded" alt="...">
      </div>
      <div class="col-md-4 bg-box">
        <form action="https://submit-form.com/gxvl-fcf-mCfZHKZ0_Rrv" target="_self">
          <input class="d-none" type="checkbox" name="_honeypot" tabindex="-1" autocomplete="off"/>
          <input type="hidden" name="_redirect" value="http://www.jmtwebdevelopment.com/contact/success"/>
          <input class="form-control d-none" type="text" name="subject" value="Client Email"/>
          <div class="form-group m-0">
            <label for="Email">Enter your Email address</label>
            <input id="Email" class="form-control" type="text" name="email" placeholder="someone@somewhere.com" required/> 
            <label for="Message" class="mt-3">Type your message</label>
            <textarea id="Message" class="form-control" name="message" rows="4" placeholder="Please contact me for a quote and any questions you may have. Thank You" required></textarea>
            <button class="btn btn-info btn-md btn-block my-3" type="submit">Send</button>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>

<% include ../partials/footer.ejs %>